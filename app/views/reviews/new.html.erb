<header class="header">
  <div class="header-body">
    <h1>New Review for <%= @restaurant.name %></h1>
  </div>
  <div class="actions text-right">
    <%= link_to raw('<i class="fas fa-chevron-left"></i> Back'), @restaurant %>
  </div>
</header>

<section class="review">
  <%= render 'form', restaurant: @restaurant, review: @review %>
</section>
